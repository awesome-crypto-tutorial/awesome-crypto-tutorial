---
layout: null
permalink: /search.json
---
[
{% assign items = site.posts %}
{% for p in items %}
  {
    "title": {{ p.title | jsonify }},
    "url": {{ p.url | relative_url | jsonify }},
    "date": "{{ p.date | date: '%Y-%m-%d' }}",
    "tags": {{ p.tags | jsonify }},
    "categories": {{ p.categories | jsonify }},
    "excerpt": {{ p.excerpt | strip_html | strip_newlines | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]


